#Write a program which accept one number form user and return addition of its factors.
#Input :12
#Output : 16 (1+2+3+4+6)

def sumfact(no):
	isum=0
	for i in range(1,no):
		if((no%i)==0):
			isum=isum+i;
	return isum

def main():
	print("enter the number")
	no=int(input());
	iRet=sumfact(no);
	print(iRet);
if __name__=="__main__":
	main();
